{
  "name": "@aevatar/kit-core",
  "version": "1.2.1",
  "description": "Core client and business logic for AevatarKit SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "dependencies": {
    "@aevatar/kit-types": "workspace:*",
    "@aevatar/kit-protocol": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aevatar/aevatarkit-ui.git",
    "directory": "packages/kit-core"
  },
  "homepage": "https://github.com/aevatar/aevatarkit-ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "aevatar",
    "agent",
    "client",
    "session",
    "sdk"
  ]
}
