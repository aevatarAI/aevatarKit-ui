{
  "name": "@aevatar/kit-types",
  "version": "0.2.0",
  "description": "Type definitions for AevatarKit SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aevatar/aevatarkit-ui.git",
    "directory": "packages/kit-types"
  },
  "homepage": "https://github.com/aevatar/aevatarkit-ui#readme",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "aevatar",
    "types",
    "typescript",
    "ag-ui",
    "a2ui"
  ]
}
